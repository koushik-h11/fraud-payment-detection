<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Hello World</h1> 
    <form method="post" action="/submit">
        <% data.forEach((item) => { %> 
            <label for="<%= item %>"><%= item %></label>
            <% if (item === "type") { %> 
                <input type="text" id="type" name="type" placeholder="Enter your <%= item %>"> 
            <% } else { %>
                <input type="number" id="<%= item %>" name="<%= item %>" placeholder="Enter your <%= item %>">
            <% } %> 
            <br>
        <% }) %>
        <button type="submit">Submit</button> 
    </form>
</body>
</html>
